            },
            {
              "subLinks": [],
              "icon": "fas fa-globe",
              "label": "Tab 2",
              "url": "javascript:alert('You selected tab 2. These tabs are only shown on the home page')"
            },
            {
              "subLinks": [],
              "icon": "fas fa-laptop-code",
              "label": "GoNative Demo",
              "url": "https://gonative.io/demo"
            }
          ],
          "id": "1"
        }
      ],
      "active": true
